{
  "name": "fbeta.fchannel-web",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "npm run prod",
    "prod": "node -r ./node_modules/module-alias/register -r dotenv/config src/.bin/www dotenv_config_path=./.env",
    "dev:be": "nodemon -i src/resources/ -i src/public/ -i src/libs/webpack-dev-server.js --exec node -r ./node_modules/module-alias/register  -r dotenv/config src/.bin/www dotenv_config_path=./.env.development",
    "dev:fe": "node -r ./node_modules/module-alias/register -r dotenv/config src/.bin/www dotenv_config_path=./.env.development",
    "build": "webpack -b --config ./webpack.prod.js",
    "webpack:dev-server": "webpack-dev-server --config ./webpack.dev.js",
    "dev": "./node_modules/.bin/concurrently -k -p [{name}] -n NODE,REACT -c red.bold,green.bold \"npm run dev:be\" \"npm run webpack:dev-server\""
  },
  "author": "FBETA",
  "license": "ISC",
