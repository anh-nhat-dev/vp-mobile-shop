    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "pino": "^6.2.1",
    "pino-pretty": "^4.0.0",
    "slug": "^2.1.1"
  },
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "@babel/preset-env": "^7.9.6",
